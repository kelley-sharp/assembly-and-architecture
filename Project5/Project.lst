					LOCAL hiAdjusted:DWORD  ; store the value of HI - LO for randomization

 000000B0  55		   *	    push   ebp
 000000B1  8B EC	   *	    mov    ebp, esp
 000000B3  83 C4 FC	   *	    add    esp, 0FFFFFFFCh
