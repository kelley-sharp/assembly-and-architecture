 000000C1  8B 5D 10				MOV   EBX, [EBP+16] ; LO
 000000C4  2B C3				SUB   EAX, EBX
 000000C6  40					INC   EAX
 000000C7  89 45 FC				MOV   hiAdjusted, EAX

 000000CA				_setupLoop:
