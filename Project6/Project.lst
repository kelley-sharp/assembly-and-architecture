 00000085  52		     1		PUSH EDX
 00000086  8B 55 08	     1		MOV  EDX, [EBP+8]
 00000089  E8 00000000 E     1		CALL WriteString
 0000008E  8B 55 10	     1		MOV  EDX, [EBP+16]
 00000091  8B 4D 14	     1		MOV  ECX, [EBP+20]  ; buffer size according to Irvine
 00000094  E8 00000000 E     1		CALL ReadString
 00000099  58		     1		POP  EAX
 0000009A  5A		     1		POP  EDX
 0000009B  59		     1		POP  ECX

						; prepare string to be looped over
 0000009C  8B 75 10				MOV  ESI, [EBP+16]
 0000009F  FC					CLD
 000000A0  B9 00000000				MOV  ECX, 0
 000000A5  C6 45 FE 01				MOV  isFirstChar, 1

 000000A9				_loadNextByte:
 000000A9  B8 00000000				MOV  EAX, 0
 000000AE  AC					LODSB
 000000AF  80 7D FE 00				CMP  isFirstChar, 0
 000000B3  74 1A				JE   _checkByte

 000000B5				_checkSignByte:
 000000B5  3C 2D				CMP  AL, 45  ; "-" character
 000000B7  74 06				JE   _hasNegativeSign
 000000B9  3C 2B				CMP  AL, 43  ; "+" character
 000000BB  74 0A				JE	_hasPositiveSign
						; otherwise jump to checkByte normally
