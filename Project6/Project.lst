			      C .LIST
			      C 
			      C 
			      C .NOLIST
			      C .LIST
			      C 
				ExitProcess proto, dwExitCode:dword

 = 00000020			MAX_STR_SIZE EQU 32
 = 0000000B			COUNT        EQU 11

				mGetString MACRO prompt, strInput, count
					; preserve registers
					PUSH EAX
					PUSH ECX
					PUSH EDX

					; display prompt
					MOV  EDX, prompt
					CALL WriteString
					; get input up to count
					MOV  EDX, strInput
					MOV  ECX, count  ; buffer size according to Irvine
					CALL ReadString

					; restore registers
					POP  EAX
					POP  EDX
					POP  ECX

				ENDM

				mDisplayString MACRO strOutput
					; preserve registers
					PUSH EDX

					; print string
					MOV  EDX, strOutput
					CALL WriteString

					; restore registers
					POP EDX

				ENDM

